#include "stdafx.h"
#include <iostream>
#include <ctime>
using namespace std;

int _tmain()
{
	    int arr[10][10];
	    srand(time(NULL));
	    for (int i = 0; i <= 10; i++)
	    {
	          	for (int j = 0; j <= 10; j++)
	          	{
	          		      arr[i][j] = 10 + rand() % 90;
	          	}
    	}
    
     	int arr1[10] = {};
    	for (int i = 0; i <= 10; i++)
	    {
	          	for (int j = 0; j <= 10; j++)
	          	{
	                		arr1[i] = arr1[i] + arr[i][j];
	          	}
    	}
	    int max = 0;
    	int num = 1;
    	for (int i = 1; i <= 10; i++)
     	{
	          	if (arr1[i] >= max) num = i;
    	}
    	cout << number;
      
    	system("pause");
    	return 0;
}
